<!doctype html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta http-equiv='X-UA-Compatible' content='IE=edge;chrome=1' />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>BucketLabs<%= ' - ' + current_article.title unless current_article.nil? %></title>
    <%= feed_tag :atom, "#{blog.options.prefix.to_s}/feed.xml", title: "Atom Feed" %>
    <%= stylesheet_link_tag 'tachyons.min.css', 'fonts.css', 'bucket-colors.css', 'bucketlabs.css' %>
  </head>
  <body class="helvetica w-100 bg-dark-gray mid-gray basiercircle">
      <section class="w-100">

<%= partial "partials/header" %>

        <div class="bg-white">
<%= yield %>
        </div>

<%= partial "partials/footer" %>

      </section>
  </body>
</html>
